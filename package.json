    "registry": "https://repositorio.bridge.ufsc.br/repository/npm-private/"
  },
  "scripts": {
    "start": "npm run clean && webpack --progress --watch",
    "clean": "shx rm -rf lib",
    "build": "npm run clean && tsc && webpack --mode=production",
    "test": "npm run lint && TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "icons": "npm run icons:clean && npm run icons:svgr && npm run icons:index",
    "icons:clean": "rm -rf ./src/components/elements/Icon/generated",
    "icons:svgr": "svgr --ext tsx --icon --template ./tools/IconTemplate.js --svgo-config svgo.config.json -d ./src/components/elements/Icon/generated ./icons",
    "icons:index": "babel-node ./tools/generateIconsIndex.js",
    "sonar": "sonar-scanner"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
