{
  "name": "bold-ui",
  "version": "1.0.0-beta.9",
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "homepage": "https://bold.bridge.ufsc.br",
  "keywords": [
    "react",
    "bold-ui",
    "bold design system"
  ],
  "files": [
    "lib",
    "icons",
    "tslint.json"
  ],
  "repository": {
    "url": "https://github.com/laboratoriobridge/bold"
  },
  "scripts": {
    "start": "npm run clean && webpack --progress --watch",
    "clean": "rm -rf lib",
    "build": "npm run clean && tsc && webpack --mode=production",
    "prepare": "npm run build",
    "test": "npm run lint && TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch --collectCoverage=false",
    "lint": "tslint -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "icons": "npm run icons:clean && npm run icons:svgr && npm run icons:index",
    "icons:clean": "rm -rf ./src/components/Icon/generated",
    "icons:svgr": "svgr --ext tsx --icon --template ./tools/IconTemplate.js --svgo-config svgo.config.json -d ./src/components/Icon/generated ./icons",
    "icons:index": "babel-node ./tools/generateIconsIndex.js",
    "sonar": "sonar-scanner",
    "site:dev": "next site/",
    "site:build": "next build site/",
    "site:sitemap": "node site/buildSitemap",
    "site:export": "next build site/ && next export site/ && touch site/out/.nojekyll && cp site/CNAME site/out/CNAME && npm run site:sitemap && build-storybook -o site/out/storybook",
    "site:deploy": "npm run site:export && gh-pages -d site/out -t"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@emotion/core": "10.0.14",
    "downshift": "3.2.10",
    "emotion": "10.0.14",
    "focus-trap-react": "6.0.0",
    "hoist-non-react-statics": "3.3.0",
    "match-sorter": "3.1.1",
    "react-dropzone": "10.1.5",
    "react-popper": "1.3.3",
    "react-text-mask": "5.4.3",
    "react-transition-group": "4.1.1",
    "text-mask-addons": "3.8.0",
    "text-mask-core": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@mdx-js/loader": "1.0.20",
    "@mdx-js/mdx": "1.0.20",
    "@mdx-js/react": "1.0.20",
    "@next/mdx": "8.1.0",
    "@storybook/addon-a11y": "5.1.9",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@svgr/cli": "4.3.0",
    "@testing-library/react": "8.0.1",
    "@testing-library/react-hooks": "1.1.0",
    "@types/classnames": "2.2.8",
    "@types/highlight.js": "9.12.3",
    "@types/hoist-non-react-statics": "3.3.1",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.134",
    "@types/match-sorter": "2.3.0",
    "@types/next": "8.0.5",
    "@types/prop-types": "15.7.1",
    "@types/react": "16.8.20",
    "@types/react-addons-create-fragment": "0.14.19",
    "@types/react-dom": "16.8.4",
    "@types/react-text-mask": "5.4.6",
    "@types/react-transition-group": "2.9.2",
    "@types/storybook__addon-a11y": "5.0.0",
